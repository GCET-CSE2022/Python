    "print(\"For Loop: Print numbers from 1 to 5\")\n",
    "for i in range(1, 6):\n",
    "    print(i, end=' ')\n",
    "print(\"\\n\")  # newline\n",
    "\n",
    "# --- While Loop ---\n",
    "print(\"While Loop: Print numbers from 1 to 5\")\n",
    "x = 1\n",
    "while x <= 5:\n",
    "    print(x, end=' ')\n",
    "    x += 1\n",
    "print(\"\\n\")"
   ]
